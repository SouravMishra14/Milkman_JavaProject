<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//-Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.beans">
    <class name="CustomerInfo" table="CustomerDetail">
         <id name="milkmanId" column="milkmanId"/>
         <property name="cId" column="cId"></property>
       <property name="cusRequirement" column="cusRequirement"></property>
        <property name="cusContact" column="cusContact"></property>
        <property name="cusAddress" column="cusAddress"></property>
        <property name="cusSubs" column="cusSubscription"></property>
        <property name="cusPayment" column="cusPayment"></property> 
    </class>
</hibernate-mapping>


